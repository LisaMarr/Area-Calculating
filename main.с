
#include<stdio.h>
#include<conio.h>

int main()
{
	 float base, height, area;
	 clrscr();
	 printf("Enter base of triangle: ");
	 scanf("%f", &base);
	 printf("Enter height of triangle: ");
	 scanf("%f", &height);
	 area = 0.5*base*height;
	 printf("Area of triangle is: %0.3f", area);
	 getch();
	 return(0);
}
